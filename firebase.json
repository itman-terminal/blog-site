{
  "functions": {
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run lint",
      "npm --prefix \"$RESOURCE_DIR\" run build"
    ]
  },
  "hosting": {
    "ignore": [
      "content",
      "firebase.js",
      "firebase.json",
      "gatsby-*.js",
      "package.json",
      "src",
      "tsconfig.json",
      "yarn.lock",
      "**/.*",
      "**/node_modules/**"
    ],
    "public": "public",
    "redirects": [
      {
        "destination": "/",
        "source": "/foo",
        "type": 302
      }
    ],
    "rewrites": [
      {
        "function": "feedRewrite",
        "source": "/feed"
      }
    ]
  }
}
